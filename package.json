    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "set NEXT_TELEMETRY_DISABLED=1 && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
